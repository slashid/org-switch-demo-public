{
  "name": "org-switch-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd client && tsc && vite build",
    "build:server": "npm run openapi",
    "dev": "npm run build && npm run serve",
    "openapi": "openapi --input https://cdn.slashid.com/slashid-openapi-latest.yaml --output ./slashid --client axios",
    "start": "ts-node ./server"
  },
  "dependencies": {
    "@fastify/jwt": "^7.2.0",
    "@fastify/static": "^6.10.2",
    "@slashid/react": "^1.10.0",
    "@slashid/slashid": "^3.12.0",
    "@vanilla-extract/css": "1.10.0",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "form-data": "^4.0.0",
    "get-jwks": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.16.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "openapi-typescript-codegen": "^0.25.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
